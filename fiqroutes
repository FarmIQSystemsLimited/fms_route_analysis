    public static final String FIGURED_FARMS_ORG =
        "rest:get:/finance/figured/farms/{organisationId}";
    public static final String FIGURED_USER_ORG =
        "rest:get:/finance/figured/user/{organisationId}";
    public static final String FIGURED_TRACKERS =
        "rest:get:/finance/figured/trackers/{farmId}";
    public static final String FIGURED_TRACKERS_FOR_CODE =
        "rest:get:/finance/figured/trackers/{organisationId}/{farmCode}";
    public static final String SEND_FIGURED_CONFIG =
        "rest:get:/finance/figured/sendconfig/{organisationId}";
    public static final String FIGURED_RUN_AGING =
        "rest:get:/finance/figured/runaging/{farmId}";
    public static final String DELETE_FIGURED_TRANSACTION =
        "rest:get:/finance/figured/delete/{financialTransactionId}/{farmId}";
    public static final String SECURITY_USER_FARM_PRIVILEGES =
        "rest:get:/security/user/{userId}/farm/{farmId}/privileges";

    public List<String> getString() {
        return Arrays.stream(TransactionType.values()).map(Enum::name).collect(Collectors.toList());
    }

}
